{
  "name": "bramwelt-patchwork",
  "version": "0.1.1",
  "author": "bramwelt",
  "summary": "Web-based patch tracking system designed to facilitate the contribution and management of contributions to open-source projects",
  "license": "Apache-2.0",
  "source": "https://github.com/bramwelt/puppet-patchwork",
  "project_page": "https://github.com/bramwelt/puppet-patchwork",
  "issues_url": "https://github.com/bramwelt/puppet-patchwork/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [ "7.0" ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [ "7.0" ]
    }
  ],
  "dependencies": [
    {
      "name":"puppetlabs-vcsrepo",
      "version_requirement":">=1.0.0 <2.0.0"
    },
    {
      "name":"puppetlabs-git",
      "version_requirement":">=0.1.0 <1.0.0"
    },
    {
      "name":"puppetlabs-mysql",
      "version_requirement":">=3.0.0 <4.0.0"
    },
    {
      "name":"stankevich-python", 
      "version_requirement":">=1.0.0 <=1.9.7"
    },
    {
      "name":"engage-uwsgi",
      "version_requirement":">=1.2.0 <2.0.0"
    },
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">=4.0.0 <5.0.0"
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": "3.x"
    },
    {
      "name": "puppet",
      "version_requirement": ">=3.1.0 <5.0.0"
    }
  ]
}

